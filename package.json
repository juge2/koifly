{
  "name": "koifly-flight-loging-app",
  "version": "0.2.0",
  "description": "Flight loging app for hang glider and paraglider pilots",
  "keywords": [
    "hapi",
    "react",
    "webpack",
    "HMR"
  ],
  "license": "MIT",
  "author": "Anastasia Kaplina <nkaplina@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaplona/koifly-flight-loging-app"
  },
  "main": "src/server.js",
  "scripts": {
    "forever_start": "NODE_ENV=production forever start src/server.js",
    "start": "NODE_ENV=production     node --harmony src/server.js",
    "build": "NODE_ENV=production     node --harmony src/scripts/make.js  &&  NODE_ENV=production   webpack -p --config src/config/webpack-config.js",
    "webpack": "NODE_ENV=development  node --harmony src/scripts/make.js  &&  NODE_ENV=development  node --harmony src/webpackDevServer.js",
    "dev": "NODE_ENV=development      node --harmony src/scripts/make.js  &&  NODE_ENV=development  nodemon --harmony src/server.js",
    "make": "NODE_ENV=development     node --harmony src/scripts/make.js",
    "db_init": "MYSQL_ADMIN=true      node --harmony src/orm/init.js",
    "db_migrate": "MYSQL_ADMIN=true      /node_modules/.bin/sequelize db:migrate",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "./node_modules/.bin/mocha --compilers jsx:babel-register --require ignore-styles ./tests/ --recursive"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.2.0",
    "babel-core": "6.25.0",
    "babel-eslint": "5.0.4",
    "babel-loader": "6.4.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.24.1",
    "bcrypt": "0.8.7",
    "chalk": "1.1.3",
    "css-loader": "0.23.1",
    "csvtojson": "1.1.9",
    "deep-extend": "0.4.2",
    "es6-promise": "3.3.1",
    "eventemitter3": "1.2.0",
    "extract-text-webpack-plugin": "1.0.1",
    "google-maps-api": "2.0.1",
    "hapi": "10.1.0",
    "hapi-auth-cookie": "3.1.0",
    "hapi-react-views": "7.0.0",
    "highcharts": "7.0.2",
    "history": "2.1.2",
    "ignore-styles": "1.2.0",
    "inert": "3.2.1",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "lodash": "3.10.1",
    "mysql": "2.9.0",
    "nodemailer": "1.11.0",
    "object.values": "1.0.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router": "2.8.1",
    "react-router-scroll": "0.2.1",
    "rimraf": "2.6.1",
    "sequelize": "3.23.0",
    "sequelize-cli": "5.1.0",
    "style-loader": "0.13.2",
    "vision": "3.0.0",
    "webpack": "1.11.0",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.3.0",
    "webpack-merge": "0.7.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "1.10.3",
    "eslint-loader": "1.8.0",
    "eslint-plugin-react": "3.16.1",
    "express": "4.13.3",
    "h2o2": "4.0.2",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "react-addons-test-utils": "15.6.0",
    "react-hot-loader": "1.3.1",
    "react-transform-hmr": "1.0.4",
    "sinon": "1.17.7",
    "sinon-chai": "2.11.0"
  },
  "peerDependencies": {
    "forever": "0.x"
  }
}
